  "type": "module",
  "dependencies": {
    "@ai-sdk/openai": "^1.1.15",
    "@octokit/webhooks": "^13.7.4",
    "ai": "^4.1.47",
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "octokit": "^4.1.2",
    "zod": "^3.24.2"
